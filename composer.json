{
    "name": "jshayes/guzzle-testing",
    "description": "A package to make it easier to test with guzzle.",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Hayes",
            "email": "justin@justinhayes.ca"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "JSHayes\\GuzzleTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\" : "tests/"
        }
    }
}
